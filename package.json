  "devDependencies": {
    "@babel/core": "^7.11.5",
    "babel-eslint": "^10.1.0",
    "babel-preset-expo": "^8.3.0",
    "eslint": "^7.8.1",
    "eslint-config-airbnb": "^18.2.0",
